{
  "locales": [
    "en",
    "zh"
  ],
  "sourceLocale": "en",
  "catalogs": [
    {
      "path": "src/locales/{locale}/messages",
      "include": [
        "src"
      ]
    }
  ],
  "formatOptions": {
    "lineNumbers": false
  },
  "fallbackLocales": {
    "default": "en"
  },
  "format": "po",
  "orderBy": "messageId",
  "pseudoLocale": "pseudo"
}